{
  "version": 1,
  "description": "Cloudflare Pages routing - serve static files directly, run middleware for dynamic routes",
  "include": ["/*"],
  "exclude": [
    "/assets/*",
    "/*.html",
    "/*.txt",
    "/*.xml",
    "/*.json",
    "/*.png",
    "/*.ico",
    "/*.jpg",
    "/*.jpeg",
    "/*.webp",
    "/*.svg",
    "/*.css",
    "/*.js",
    "/*.map",
    "/.well-known/*"
  ]
}
